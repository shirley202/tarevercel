{
    "builds": [{
        "src": "python_heroku_task/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "Python 3.9.7"}
        }],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "python_heroku_task/wsgi.py"
        }
    ]
}